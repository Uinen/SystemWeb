@model IEnumerable<SystemWeb.Models.RagioneSociale>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "RagioneSocialeCreate")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Nome)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Nome)
        </td>
        <td>
            @Html.ActionLink("Edit", "RagioneSocialeEdit", new { id = item.RagioneSocialeId }) |
            @Html.ActionLink("Details", "RagioneSocialeDetails", new { id = item.RagioneSocialeId }) |
            @Html.ActionLink("Delete", "RagioneSocialeDelete", new { id = item.RagioneSocialeId })
        </td>
    </tr>
}

</table>
